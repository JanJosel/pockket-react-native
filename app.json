{
  "expo": {
    "name": "Pockket",
    "slug": "pockket-mobile",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.g1corp.pockket",
      "versionCode": 1
    },
    "description": "Pockket, your mobile e-payment companion",
    "githubUrl": "https://github.com/JanJosel/pockket-react-native"
  }
}
